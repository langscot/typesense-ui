{
  "editor.tabSize": 2,
  "editor.defaultFormatter": "biomejs.biome",
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports": "explicit"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "conventionalCommits.scopes": [
    "collections"
  ],
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "latex",
    "plaintext"
  ]
}
